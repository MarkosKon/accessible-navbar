  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "react",
    "styled-components",
    "accessible",
    "navbar"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkosKon/accessible-navbar.git"
